        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/TosTheme"
        android:hardwareAccelerated="false"
        >
        <activity android:name=".LoginActivity">
        </activity>
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".VideoActivity">
        </activity>
        <activity android:name=".BrushListActivity">
        </activity>
        <activity android:name=".AddBrushListActivity">
        </activity>
        <activity android:name=".QnAActivity">
        </activity>
        <activity android:name=".CalendarActivity">
        </activity>
        <activity android:name=".InformationActivity">

        </activity>
        <activity android:name=".ModeActivity">
        </activity>
        <activity android:name=".SelfCheckActivity">
        </activity>
        <activity android:name=".IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>
    <queries>
        <package android:name="com.kakao.talk" />
    </queries>

</manifest>
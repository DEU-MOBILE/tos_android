<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="28dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nanumsquareeb"
                android:text="강호동"
                android:textColor="@color/ssamocolor"
                android:textSize="36sp" />

            <TextView
                android:id="@+id/tv_static_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nanumsquarer"
                android:text="@string/static_text2"
                android:textColor="@color/ssamocolor"
                android:textSize="36sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_list_item_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nanumsquareeb"
                android:text="칫솔"
                android:textColor="@color/ssamocolor"
                android:textSize="36sp" />

            <TextView
                android:id="@+id/tv_static_2"
                android:layout_width="46dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="248dp"
                android:layout_marginBottom="4dp"
                android:fontFamily="@font/nanumsquarer"
                android:text="@string/static_text3"
                android:textColor="@color/ssamocolor"
                android:textSize="36sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nanumsquareeb"
                android:text="38"
                android:textColor="@color/ssamocolor"
                android:textSize="36sp" />

            <TextView
                android:id="@+id/tv_static_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nanumsquarer"
                android:text="@string/static_text4"
                android:textColor="@color/ssamocolor"
                android:textSize="36sp" />

        </LinearLayout>

    </LinearLayout>

    <ImageView
        android:id="@+id/image_view"
        android:layout_width="371dp"
        android:layout_height="65dp"
        android:layout_gravity="center"
        android:layout_marginTop="548dp"
        android:background="@drawable/ssambutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="46dp"
        android:layout_height="31dp"
        android:layout_gravity="center"
        android:layout_marginStart="375dp"
        android:layout_marginTop="572dp"
        android:layout_marginEnd="300dp"
        android:fontFamily="@font/nanumsquareeb"
        android:text="확인"
        android:textColor="#ffffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.574"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <org.eazegraph.lib.charts.BarChart
        android:id="@+id/barchart2"
        android:fontFamily="@font/nanumsquareb"
        android:layout_width="match_parent"
        android:layout_height="256dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="250dp"
        android:layout_marginEnd="20dp"
        android:background="@color/chartbackground"
        android:padding="10dp"
        app:egBarWidth="20dp"
        app:egFixedBarWidth="true"
        app:egLegendHeight="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquarer"
        android:text="오늘의 양치 점수"
        android:textColor="#ff98BFBD"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="224dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>
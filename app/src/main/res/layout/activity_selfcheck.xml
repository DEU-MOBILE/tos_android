<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">



    <CheckBox
        android:id="@+id/checkbox1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="273dp"
        android:background="@drawable/checkbox_checkbox"
        android:button="@android:color/transparent"
        android:checked="true"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="319dp" />

    <CheckBox
        android:id="@+id/checkbox2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="29dp"
        android:background="@drawable/checkbox_checkbox"
        android:button="@android:color/transparent"
        android:checked="true"
        app:layout_constraintTop_toBottomOf="@+id/checkbox1"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="319dp" />

    <CheckBox
        android:id="@+id/checkbox3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="29dp"
        android:background="@drawable/checkbox_checkbox"
        android:button="@android:color/transparent"
        android:checked="true"
        app:layout_constraintTop_toBottomOf="@+id/checkbox4"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="319dp" />

    <CheckBox
        android:id="@+id/checkbox4"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="29dp"
        android:background="@drawable/checkbox_checkbox"
        android:button="@android:color/transparent"
        android:checked="true"
        app:layout_constraintTop_toBottomOf="@+id/checkbox2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="319dp" />

    <TextView
        android:id="@+id/textView11"
        android:fontFamily="@font/nanumsquareb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="양치를 하는데 이가 시려요."
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="278dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView12"
        android:fontFamily="@font/nanumsquareb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="양치를 하는데 이가 흔들려요."
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="332dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView13"
        android:fontFamily="@font/nanumsquareb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="양치를 하는데 이가 아파요."
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="391dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView14"
        android:fontFamily="@font/nanumsquareb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="양치를 하는데 피가 났어요."
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="450dp"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nanumsquarer"
        android:text="양치 자가진단표"
        android:textColor="#ff98BFBD"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="226dp" />
    <!-- //체크박스 -->

    <TextView
        android:id="@+id/textView18"
        android:layout_width="276dp"
        android:layout_height="74dp"
        android:fontFamily="@font/nanumsquareeb"
        android:text="강호동 님은 N 번째 양치를 완료 했어요!"
        android:textColor="#ff98BFBD"
        android:textSize="32sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="74dp" />

    <ImageView
        android:id="@+id/image_view"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:layout_gravity="center"
        android:layout_marginTop="548dp"
        android:background="@drawable/ssambutton"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_gravity="center"
        android:layout_marginStart="375dp"
        android:layout_marginTop="580dp"
        android:layout_marginEnd="300dp"
        android:fontFamily="@font/nanumsquareeb"
        android:text="제출하기"
        android:textColor="#ffffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>
